<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="supplier_company_code" model="ir.sequence">
            <field name="name">Supplier Company Code</field>
            <field name="code">supplier.company.code</field>
            <field name="prefix">SC</field>
            <field name="padding">6</field>
        </record>

        <record id="global_vendor_code" model="ir.sequence">
            <field name="name">Global Vendor Code</field>
            <field name="code">global.vendor.code</field>
            <field name="prefix">GV</field>
            <field name="padding">6</field>
        </record>

        <record id="supplier_company_user" model="ir.sequence">
            <field name="name">Supplier Company User</field>
            <field name="code">supplier.company.user</field>
            <field name="padding">5</field>
        </record>

        <record id="supplier_vendor_code" model="ir.sequence">
            <field name="name">Supplier Vendor Code</field>
            <field name="code">supplier.vendor.code</field>
            <field name="padding">7</field>
        </record>

        <record id="score_definition_code" model="ir.sequence">
            <field name="name">Score Definition Code</field>
            <field name="code">score.definition.code</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>