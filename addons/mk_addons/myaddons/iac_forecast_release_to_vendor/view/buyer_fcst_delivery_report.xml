<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
      <!--MM 下载rfq的向导-->
      <record id="view_iac_buyer_fcst_delivery_report_form" model="ir.ui.view">
          <field name="name">iac.buyer.fcst.delivery.report.wizard</field>
          <field name="model">iac.buyer.fcst.delivery.report.wizard</field>
          <field name="arch" type="xml">
              <form string="Wizard tools">
                  <group>
                      <field name="buyer_id" options="{'no_create': True, 'no_edit': True}" required="1"/>
                      <field name="division_id" options="{'no_create': True, 'no_edit': True}"/>
                      <field name="vendor_id" options="{'no_create': True, 'no_edit': True}"/>
                  </group>

                  <footer>
                      <button name="action_confirm" string="Confirm" type="object"  class="oe_highlight"  />
                      or
                      <button string="Cancel" class="oe_link" special="cancel" />
                  </footer>
              </form>
          </field>
      </record>

      <record model="ir.actions.act_window"  id="action_iac_buyer_fcst_delivery_report_form">
          <field name="name">iac.buyer.fcst.delivery.report.wizard</field>
          <field name="res_model">iac.buyer.fcst.delivery.report.wizard</field>
          <field name="view_type">form</field>
          <field name="view_mode">form</field>
          <field name="target">new</field>
      </record>


  </data>
</odoo>